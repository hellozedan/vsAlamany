@model IEnumerable<almany.AfterBeforeImage>
<div id="carousel">
    <div id="owl-demo" class="owl-carousel owl-theme">
        @foreach (var item in Model)
        {
            <div class="item">
                <img src="@item.url_picture_before" alt="@item.first_title">
                <img src="@item.url_picture_after" alt="@item.secoundry_title">
                <div class="carousel-text">
                    <div class="line">
                        <div class="s-12 l-9 before">
                            <h2>לפני</h2>
                        </div>
                        <div class="s-12 l-9 after">
                            <h2>אחרי</h2>
                        </div>
                    </div>
                </div>
            </div>
        }
    </div>
</div>